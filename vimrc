map <C-t> <esc>:tabnew<CR>
map <C-w> <esc>:w<CR>
imap <C-w> <esc>:w<CR>

set t_Co=256
syntax on
filetype off
filetype plugin indent on
set background=light
colorscheme elflord
set wildmenu
set wildmode=list:full
set smarttab
set noincsearch
set ignorecase smartcase
set laststatus=2
set number
set list
set listchars=tab:……,trail:░,extends:>,precedes:<
set smartindent
set tabstop=2
set shiftwidth=2
set expandtab
set hlsearch
